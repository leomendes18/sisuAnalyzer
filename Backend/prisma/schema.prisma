generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model sisu2016 {
  id             Int     @id @default(autoincrement())
  ano            Int?
  edicao         Int?
  uf_campus      String?
  codigo_curso   Int?
  nome_curso     String?
  grau           String?
  sexo           String?
  nota_candidato Float?  @db.Real
  nota_corte     Float?  @db.Real
  aprovado       String?
}

